{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "lib": ["ESNext"],
    "types": [
      "vite/client",
      "@cloudflare/vitest-pool-workers",
      "./worker-configuration.d.ts"
    ],
    "paths": {
      "@/*": ["./src/*"],
      "@test/*": ["./test/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx"
  }
}
